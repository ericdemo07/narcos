
<h1>Listing users</h1>

<%= link_to 'New User', new_user_path %>

<table>
  <tr>
    <th>Email</th>
    <th>Phone</th>
    <th>AadharId</th>
    <th>Password</th>
  </tr>

  <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.phone %></td>
        <td><%= user.aadharid %></td>
        <td><%= user.password %></td>

        <td><%= link_to 'Show', user_path(user) %></td>

      </tr>
  <% end %>
</table>
